{
  "icons": {
    "16": "./gen-assets/icon16.plasmo.png",
    "32": "./gen-assets/icon32.plasmo.png",
    "48": "./gen-assets/icon48.plasmo.png",
    "64": "./gen-assets/icon64.plasmo.png",
    "128": "./gen-assets/icon128.plasmo.png"
  },
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "./gen-assets/icon16.plasmo.png",
      "32": "./gen-assets/icon32.plasmo.png",
      "48": "./gen-assets/icon48.plasmo.png",
      "64": "./gen-assets/icon64.plasmo.png",
      "128": "./gen-assets/icon128.plasmo.png"
    },
    "default_popup": "./popup.html"
  },
  "version": "0.0.1",
  "author": "Michael Deglin",
  "name": "Cupid bot",
  "description": "Extension that helps to automate the matching process in the OkCupid app",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.okcupid.com/*"
      ],
      "js": [
        "../src/contents/main.ts"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "https://www.okcupid.com/*"
      ],
      "js": [
        "static/contents/index.tsx"
      ]
    }
  ],
  "host_permissions": [
    "https://*/*"
  ]
}
